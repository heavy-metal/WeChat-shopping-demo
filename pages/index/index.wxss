/**index.wxss**/
.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cates {
  display: flex;
}
.index_cates navigator {
  padding: 20rpx;
  flex: 1;
}
.index_cates navigator image {
  width: 100%;
}
.index_floor .floor_group .floor_title {
  padding: 10rpx 0;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
}
.index_floor .floor_group .floor_content {
  overflow: hidden;
}
.index_floor .floor_group .floor_content navigator {
  float: left;
  width: 33.33%;
}
.index_floor .floor_group .floor_content navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_group .floor_content navigator:nth-child(2),
.index_floor .floor_group .floor_content navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_group .floor_content navigator:nth-child(3),
.index_floor .floor_group .floor_content navigator:nth-child(5) {
  border-right: 10rpx solid #fff;
}
.index_floor .floor_group .floor_content navigator:nth-child(1) {
  border-left: 10rpx solid #fff;
}
.index_floor .floor_group .floor_content navigator:nth-child(1) image {
  height: 55.45422414vw;
}
.index_floor .floor_group .floor_content navigator image {
  width: 100%;
  height: 100%;
}
